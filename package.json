{
  "name": "@js-basics/vector",
  "version": "1.41.1",
  "description": "A vector lib with support for (+ - * /) operator handling",
  "main": "./index.js",
  "module": "./build/esm/index.js",
  "directories": {
    "build": "build"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc npx mocha --recursive",
    "lint": "eslint src",
    "build": "cross-env BABEL_ENV=production npx rollup --config ./.rolluprc",
    "docs": "npx typedoc ./src --out ./docs --allowJs --checkJs --noEmit --excludeNotExported --module commonjs --target es6 --exclude **/debug.js",
    "prepare": "npm run clean && npm run test && npm run build && npm run docs",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/basics/vector.git"
  },
  "keywords": [
    "vector",
    "operator",
    "math",
    "arithmetic",
    "overload",
    "basic",
    "vec3",
    "vec2",
    "valueOf"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basics/vector/issues"
  },
  "homepage": "https://github.com/basics/vector#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-minify": "^0.5.1",
    "babelrc-rollup": "^3.0.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^15.13.31"
  },
  "snyk": true
}
